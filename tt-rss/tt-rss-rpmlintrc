addFilter("tt-rss.noarch: W: package-with-huge-docs")
addFilter("tt-rss.noarch: W: zero-length")
addFilter("tt-rss.noarch: W: zero-length")
addFilter("tt-rss.noarch: W: file-not-in-%lang")
addFilter("tt-rss.noarch: W: non-executable-script")
addFilter("tt-rss.noarch: W: suse-missing-rclink")
